{
    "family": "balance-server",
    "volumes": [],
    "containerDefinitions": [
        {
            "environment": [],
            "name": "nginx",
            "image": "balance/nginx:staging",
            "cpu": 10,
            "memory": 500,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "essential": true
        },
	{
            "environment": [],
            "name": "web",
            "image": "balance/web:staging",
            "cpu": 10,
            "memory": 500,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 3031,
                    "hostPort": 3031
                },
                {
                    "containerPort": 9191,
                    "hostPort": 9191
                },
            ],
            "environment": [
                {
                    "name": "HTTPS_ENABLED",
                    "value": "True"
                },
                {
                    "name": "APP_CONFIG",
                    "value": "development"
                },
                {
                    "name": "DATABASE_URL",
                    "value": "postgresql://postgres@db/postgres"
                },
                {
                    "name": "SECRET_KEY",
                    "value": "testing-secret-key"
                },
                {
                    "name": "REDIS_URL",
                    "value": "redis://redis"
                },
                {
                    "name": "C_FORCE_ROOT",
                    "value": "true"
                },
                {
                    "name": "CELERY_BROKER_URL",
                    "value": "amqp://guest@rabbit"
                },
                {
                    "name": "CELERY_RESULT_BACKEND",
                    "value": "redis://redis"
                },
                {
                    "name": "STATIC_S3_BUCKET_NAME",
                    "value": "mock-bucket-name"
                }
            ]
        },
	{
            "environment": [],
            "name": "db",
            "image": "balance/db:staging",
            "cpu": 10,
            "memory": 500,
            "essential": true,
            "environment": [
                {
                    "name": "HTTPS_ENABLED",
                    "value": "True"
                },
                {
                    "name": "APP_CONFIG",
                    "value": "development"
                },
                {
                    "name": "DATABASE_URL",
                    "value": "postgresql://postgres@db/postgres"
                },
                {
                    "name": "SECRET_KEY",
                    "value": "testing-secret-key"
                },
                {
                    "name": "REDIS_URL",
                    "value": "redis://redis"
                },
                {
                    "name": "C_FORCE_ROOT",
                    "value": "true"
                },
                {
                    "name": "CELERY_BROKER_URL",
                    "value": "amqp://guest@rabbit"
                },
                {
                    "name": "CELERY_RESULT_BACKEND",
                    "value": "redis://redis"
                },
                {
                    "name": "STATIC_S3_BUCKET_NAME",
                    "value": "mock-bucket-name"
                }
            ]
        },
	{
            "environment": [],
            "name": "redis",
            "image": "redis",
            "cpu": 10,
            "memory": 500,
            "essential": true
        },
	{
            "environment": [],
            "name": "rabbit",
            "image": "rabbitmq:3",
            "cpu": 10,
            "memory": 500,
            "essential": true
        }
    ]
}